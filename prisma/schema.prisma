// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String         @id @default(cuid())
  name String?
  email String
  password String?
  dob DateTime?
  profession String?
  country String?
  state String?
  city String?
  gender String?
  role String?
  createdAt DateTime @default(now())
}